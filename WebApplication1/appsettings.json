{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "DataSource": {
    "DataSourceType": "DatabaseFirst",
    "CacheExpiryMinutes": 5,
    "EnableCache": true,
    "EnableDatabase": true,
    "WarmupStrategy": "OnDemand",
    "DatabaseTimeoutSeconds": 60,
    "EnableDetailedLogging": false
  }
}
